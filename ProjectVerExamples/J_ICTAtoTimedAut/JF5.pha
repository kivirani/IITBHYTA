timedautomaton F5a
contr_var: x;
input_var: y;
synclabs: a,b;
loc s1: while true
	when true sync a do {x'==0} goto s2;
loc s2: while true
	when y>3 sync b goto s1;
initially: s1;
end

timedautomaton F5b
contr_var: y;
input_var: x;
synclabs: c,d;
loc s3: while true
	when true sync c do {y'==0} goto s4;
loc s4: while true
	when x>5 sync d goto s3;
initially: s3;
end

//F5a.print;
F5b.print;
